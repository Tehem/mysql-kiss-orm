{
  "name": "mysql-kiss-orm",
  "version": "0.4.5",
  "description": "Simple Node.JS MySQL ORM attempt",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=8.12"
  },
  "scripts": {
    "coverage": "LOGGER_LEVEL=fatal NODE_ENV=test nyc _mocha",
