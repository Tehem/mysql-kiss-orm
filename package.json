    "mocha": "NODE_ENV=test mocha --bail",
    "postinstall": "npm run esdoc",
    "prettier": "prettier -l '{lib,test}/**/*.{js,yml,yaml,json}'",
    "test": "npm run lint && npm run coverage"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "mysql2": "1.6.1"
  },
  "devDependencies": {
